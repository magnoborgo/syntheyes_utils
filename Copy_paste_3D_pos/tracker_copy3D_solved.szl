//SIZZLET mb Tracker - Copy 3D Solved Position
//by Magno Borgo 2021


ob = Scene.activeObj
trk = ob.trk

Global.trckX = 0
Global.trckY = 0
Global.trckZ = 0 
Global.trckVector = 0
Global.trckLock = 0

for (tk in trk)
    if (tk.isSel)
    Global.trckX = tk.wx
	Global.trckY = tk.wy
	Global.trckZ = tk.wz
	Global.trckVector = tk.wvec
	Global.trckLock = tk.ovec
    end
end
